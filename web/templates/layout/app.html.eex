<html lang="en">
    <head>
        <title><%= @title %> - alisinabh</title>
        <meta name="viewport" content="width=device-width, initial-scale=1">
        <link rel="stylesheet" type="text/css" href="<%= static_path(@conn, "/css/app.css") %>"/>
        <link href="https://fonts.googleapis.com/css?family=Inconsolata" rel="stylesheet">
        <meta name="description" content="@alisinabh Tech blog">
        <meta name="author" content="Alisina Bahadori">
        <meta name="keywords" content=", tech, devops, elixir, cloud, geeky stuff">
        <link rel="alternate" type="application/rss+xml" title="RSS Feed for alisinabh.com" href="http://alisinabh.com/feed" />
        <meta property="og:description" content="@alisinabh blog about my tech life as a programmer and my experiences.">
        <meta property="og:type" content="blog">
        <meta property="og:title" content="Alisinabh Tech blog">
        <meta property="og:url" content="/">
        <meta property="og:site_name" content="Alisinabh Tech blog">
        <meta name="twitter:card" content="summary">
        <meta name="twitter:title" content="@alisinabh Tech Blog">
        <meta name="twitter:description" content="@alisinabh blog about my tech life as a programmer.">
        <meta name="twitter:creator" content="@alisinabh">

        <link rel="apple-touch-icon" sizes="180x180" href="/apple-touch-icon.png">
        <link rel="icon" type="image/png" href="/favicon-32x32.png" sizes="32x32">
        <link rel="icon" type="image/png" href="/favicon-16x16.png" sizes="16x16">
        <link rel="manifest" href="/manifest.json">
        <link rel="mask-icon" href="/safari-pinned-tab.svg" color="#282c34">
        <meta name="theme-color" content="#282c34">
    </head>
    <body>

        <div style="margin: 15px 0 0 0; padding:0 15px;">
            <a href="https://alisinabh.com">
                <h1 class="c-syn">defmodule
                    <span class="c-mod">Alisinabh.Blog</span>
                    do</h1>
            </a>

            <%= render @view_module, @view_template, assigns %>

            <h1 class="c-syn">end</h1>
        </div>

        <div id="mod-connect" style="margin: 4em 0 0 0; padding:0 15px;">
            <h1 class="c-syn">defmodule
                <span class="c-mod">Alisinabh.Contact</span>
                do</h1>

            <div id="articles">
                <article class="soc-article">
                    <h3>
                        <span>def</span>
                        <a href="https://github.com/alisinabh">github</a>
                        <span>do:
                            <span class="c-nor">
                                <a href="https://github.com/alisinabh">"@alisinabh"</a>
                            </span>
                        </span>
                    </h3>
                </article>
                <article class="soc-article">
                    <h3>
                        <span>def</span>
                        <a href="https://twitter.com/alisinabh">twitter</a>
                        <span>do:
                            <span class="c-nor">
                                <a href="https://twitter.com/alisinabh">"@alisinabh"</a>
                            </span>
                        </span>
                    </h3>
                </article>
                <article class="soc-article">
                    <h3>
                        <span>def</span>
                        <a href="mailto:alisina.bm@gmail.com">email</a>
                        <span>when
                            <span class="c-nor">is_human(client)</span>
                            do:
                            <span class="c-nor">
                                <a href="mailto:alisina.bm@gmail.com">"alisina.bm@gmail.com"</a>
                            </span>
                        </span>
                        <i class="art-date"># guard for bots :|
                        </i>
                    </h3>
                </article>
            </div>
            <h1 class="c-syn">end</h1>
        </div>
        <script>
          (function(i,s,o,g,r,a,m){i['GoogleAnalyticsObject']=r;i[r]=i[r]||function(){
          (i[r].q=i[r].q||[]).push(arguments)},i[r].l=1*new Date();a=s.createElement(o),
          m=s.getElementsByTagName(o)[0];a.async=1;a.src=g;m.parentNode.insertBefore(a,m)
          })(window,document,'script','https://www.google-analytics.com/analytics.js','ga');

          ga('create', 'UA-96645473-1', 'auto');
          ga('send', 'pageview');

        </script>
    </body>
</html>
